    "less-loader": "^10.2.0",
    "lint-staged": "^12.1.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
